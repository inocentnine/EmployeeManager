<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ncu.ies.employee.domain.EmployeeEntity" table="employee" schema="employeemanager">
        <id name="eid" column="eid"/>
        <property name="ename" column="ename"/>
        <property name="birth" column="birth"/>
        <property name="address" column="address"/>
        <property name="participationDate" column="participation_date"/>
        <property name="tele" column="tele"/>
        <property name="technicalLevel" column="technical_level"/>
        <property name="monthlySalary" column="monthly_salary"/>
        <property name="username" column="username"/>
        <property name="password" column="password"/>
    </class>
</hibernate-mapping>